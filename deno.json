{
  "name": "@raptor/validator",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "publish": {
    "exclude": [
      ".github",
      "examples",
      "docs",
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "test": "APP_URL=http://localhost:8000 deno test --coverage=coverage-report --allow-all --parallel",
    "test-coverage": "APP_URL=http://localhost:8000 deno coverage coverage-report"
  },
  "fmt": {
    "exclude": [
      "README.md"
    ]
  },
  "lock": true,
  "imports": {
    "@raptor/framework": "jsr:@raptor/framework@0.13.0",
    "@raptor/error": "jsr:@raptor/error@0.7.1",
    "@raptor/router": "jsr:@raptor/router@^0.8.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "shiki": "npm:shiki@^3.20.0",
    "ventojs": "npm:ventojs@^2.2.0"
  }
}
